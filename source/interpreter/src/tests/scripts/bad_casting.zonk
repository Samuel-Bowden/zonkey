function impossible_casts() {
	let a = Integer println("Hello");
	let b = Float println("Hello");
	let c = String println("Hello");
	let d = Boolean println("Hello");
}

function pointless_casts() {
	let a = Integer 1 + 2 + 3;
	let b = Float 1.1 + 2.4 + 8.2 + 8.;
	let c = String "Hello" + "Hello";
	let d = Boolean true;
}

start {
	impossible_casts();
	pointless_casts();
}
