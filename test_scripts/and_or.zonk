start {
	cli:println("------And Test------");

	if (true & true) {
		cli:println("Passed Test 1");
	}

	if (!(true & false)) {
		cli:println("Passed Test 2");
	}

	if (!(false & true)) {
		cli:println("Passed Test 3");
	}

	if (!(false & false)) {
		cli:println("Passed Test 4");
	}

	if (!(true & true & true & false)) {
		cli:println("Passed Test 5");
	}

	if (true & true & true & true & true & true) {
		cli:println("Passed Test 6");
	}

	cli:println("------Or Test------");

	if (true | true) {
		cli:println("Passed Test 1");
	}

	if (true | false) {
		cli:println("Passed Test 2");
	}

	if (false | true) {
		cli:println("Passed Test 3");
	}

	if (!(false | false)) {
		cli:println("Passed Test 4");
	}

	if (false | true | false | false | true) {
		cli:println("Passed Test 5");
	}

	if (!(false | false | false | false | false | false)) {
		cli:println("Passed Test 6");
	}
}
