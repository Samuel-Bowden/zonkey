start {
	let password = "";
	let attempts = 0;

	cli::println("You have three login attempts");

	loop {
		password = cli::prompt("Enter your password:");
		if (password == "1234") {
			cli::println("Welcome");
			break;
		} else if (attempts == 2) {
			cli::println("3 attempts made, locking account");
			break;
		} else {
			cli::println("Wrong password");
			attempts += 1;
		}
	}
}
