function preemptiveHappyBirthday(String first, String age) {
	cli:println("Happy birthday " + first + " for when you are " + age + " years old!");
}

function energy(Integer power, Integer time) -> Integer {
	let constant = 2;
	return power * time * constant;
}

function two_greater(Integer a, Integer b) -> Boolean {
	if (a - b == 2) {
		return true;
	}

	return false;
}

start {
	preemptiveHappyBirthday("Sam", "22");

	cli:println(energy(10, 20));

	let a = 1;
	let b = 2;

	if (two_greater(a, b)) {
		cli:println("a - b == 2");
	} else {
		cli:println("a - b != 2");
	}

	b -= 2;
	a += 1;

	if (two_greater(a, b)) {
		cli:println("a - b == 2");
	} else {
		cli:println("a - b != 2");
	}
}
