#!/usr/bin/env zonkey

# Test that unix shebangs work correctly

start {
	cli::println("First 10 elements of Fibonacci Sequence: ");

	let n1 = 0;
	let n2 = 1;

	for (let i = 0, i < 10, i += 1) {
		cli::print(n1);
		let nth = n1 + n2;
		n1 = n2;
		n2 = nth;

		if (i != 9) {
			cli::print(", ");
		}
	}

	cli::println("");
}
